public class Lab1 {
    // --------------------------
    //  main prints the list
    // --------------------------
    public static void main (String[] args) {
        System.out.println ();
        System.out.println (" \tName\t\tHometown") ;
        System.out.println ("\t====\t\t========");
        System.out.println ("\tSally\t\tRoanoke");
        System.out.println ("\tAlexander\tWashington");
        System.out.println ();
    }
}